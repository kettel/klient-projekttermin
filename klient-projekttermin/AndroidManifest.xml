<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.klien_projekttermin"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-feature android:name="android.hardware.camera"/>
    <user-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/loggakrona"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="map.MapActivity"
            android:configChanges="orientation"
            android:label="@string/title_activity_map" >
        </activity>
        <activity
            android:name="messageFunction.CreateMessage"
            android:label="@string/title_activity_create_new_message" >
        </activity>
        <activity
            android:name="messageFunction.StartsidaMeddelanden"
            android:label="@string/title_activity_startsida_meddelanden" >
        </activity>
        <activity
            android:name="messageFunction.Inbox"
            android:label="@string/title_activity_inbox" >
            
        </activity>
		<activity
            android:name="camera.Camera"
            android:label="@string/title_activity_camera" >
        </activity>
        <activity
            android:name="logger.LogViewer"
            android:label="@string/title_activity_log_viewer" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.klien_projekttermin.MainActivity" />
        </activity>
        <activity
            android:name="messageFunction.DisplayOfConversation"
            android:label="@string/title_activity_display_of_conversation" >
        </activity>
        <activity
            android:name="messageFunction.DisplayMessage"
            android:label="@string/title_activity_message_view" >
        </activity>
        <service android:name="communicationModule.CommunicationService">
    </service>
    </application>
</manifest>
