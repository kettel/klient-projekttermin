<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.klien_projekttermin"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="messageFunction.CreateNewMessage"
            android:label="@string/title_activity_create_new_message" >
        </activity>
        <activity
            android:name="messageFunction.StartsidaMeddelanden"
            android:label="@string/title_activity_startsida_meddelanden" >
            
            
           
        </activity>
        <activity
            android:name="messageFunction.Inbox"
            android:label="@string/title_activity_inbox">
            
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
        </activity>
        <activity
            android:name="logger.LogViewer"
            android:label="@string/title_activity_log_viewer" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.klien_projekttermin.MainActivity" />
        </activity>
        <activity
            android:name="messageFunction.DisplayOfConversation"
            android:label="@string/title_activity_display_of_conversation"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name="messageFunction.DisplayMessage"
            android:label="@string/title_activity_message_view" >
        </activity>
    </application>

</manifest>